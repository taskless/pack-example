{
  "request": {
    "url": "https://example.com/api",
    "method": "POST",
    "headers": [["Content-Type", "application/json"]],
    "body": "{\"a\":1,\"b\":2}"
  },
  "response": {
    "status": 200,
    "statusText": "OK",
    "headers": [["Content-Type", "application/json"]],
    "body": "{\"c\":3,\"d\":4}"
  },
  "tests": [
    {
      "name": "preTest is set",
      "test": "$[*].dimensions[?(@.name == 'testpack/testPre' && @.value == 'preTest')]"
    },
    {
      "name": "postTest is set",
      "test": "$[*].dimensions[?(@.name == 'testpack/testPost' && @.value == 'postTest')]"
    }
  ]
}
